# Build outputs (will be generated)
build/
dist/
.next/
out/

# Environment files (use Railway environment variables instead)
.env
.env.local
.env.development.local
.env.test.local
.env.production.local

# Logs
npm-debug.log*
yarn-debug.log*
yarn-error.log*
pnpm-debug.log*
lerna-debug.log*

# OS
.DS_Store
*.pem
*.swp
*.swo
*~

# IDE
.vscode/
.idea/

# Database files
*.db
*.sqlite

# Testing
coverage/
.nyc_output/

# Docker (not needed for Railway)
docker-compose.yml
DOCKER.md
backend/Dockerfile
frontend/Dockerfile

# Documentation (optional, but can exclude for smaller builds)
